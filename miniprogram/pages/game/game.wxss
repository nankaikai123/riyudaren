.jindutiao {
  border: 5rpx solid #777;
  background-color: white;
  position: absolute;
  border-radius: 20rpx;
  width:620rpx;
  height: 28rpx;
  top: 75%;
  left: 8%;
}
.jindutiao2 {
  background-color: #73c944;
  
  height: 28rpx;
  border-radius: 25rpx;
  overflow: hidden;
}
.daTiQu{
  background-color: gray;
  width: 98%;
  height: 60%;
